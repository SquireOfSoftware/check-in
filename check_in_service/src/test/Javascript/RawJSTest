
// Injecting javascript functionality into a basic html page to ensure that results can be extracted out and parsed
document.getElementById("test").onclick = function () {var xhttp = new XMLHttpRequest();
xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
       // Typical action to be performed when the document is ready:
        var response = xhttp.responseText;
        console.log("ok"+response);
    }
};
xhttp.open("GET", "http://localhost:8080/persons", true);

xhttp.send();}